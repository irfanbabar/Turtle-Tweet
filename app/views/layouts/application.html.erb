<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= render "layouts/stylesheets"%>
</head>
<body>
	 <div class="container">
		 <%= render "layouts/headers"%>
		 <section class="round">
		 <% flash.each do |key, value|%>
		 	 <div class="flash <%=key%>"><%= value %></div>
		 <% end %>
			 <%= yield %>
		 </section>
	 	 <%= render :partial=>"layouts/footers"%>
	 	 <%= debug params if Rails.env.development?%>
	 </div>      
</body>
</html>

